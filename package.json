    "db:seed:verbose": "tsx src/scripts/seed.ts --verbose",
    "db:seed:dry-run": "tsx src/scripts/seed.ts --dry-run",
    "db:seed:check": "tsx src/scripts/seed.ts --check",
    "notifications:generate": "tsx scripts/notifications/run-generators.ts",
    "docker:build": "docker build -t av-rentals:latest .",
    "docker:build-push": "bash scripts/docker-build-push.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f app",
    "docker:down": "docker-compose down"
