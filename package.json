{
  "type": "module",
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fast": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:api": "vitest run src/__tests__/api.integration.test.ts --reporter=verbose",
    "test:api:watch": "vitest watch src/__tests__/api.integration.test.ts",
    "test:forms": "vitest run src/__tests__/forms.integration.test.ts --reporter=verbose",
